    <title>Adding User</title>
</head>
<body>
<h1>Adding User</h1>
<form action="" method="post">
    <label for="usrs_name">User Name</label>
    <input type="text" name="name" id="usrs_name" value="<%= data.name %>" /> <br><br>

    <label for="usrs_userName">User UserName</label>
    <input type="text" name="userName" id="usrs_userName" value="<%= data.userName %>" /> <br><br>

    <label for="usrs_password">User Password</label>
    <input type="text" name="password" id="usrs_password" value="<%= data.password %>" /> <br><br>

    <label for="usrs_email">User email</label>
    <input type="text" name="email" id="usrs_email" value="<%= data.email %>" /> <br><br>

    <label for="usrs_typeID">User typeID</label>
    <input type="text" name="typeID" id="usrs_typeID" value="<%= data.typeID %>" /> <br><br>

    <label for="usrs_name">Student ID</label>
    <input type="text" name="StudentID" id="usrs_StudentID" value="<%= data.StudentID %>" /> <br><br>


    <button name="btn" value="1">send</button>
</form>
</body>
</html>